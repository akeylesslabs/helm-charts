        {
            "id": 4,
            "definition": {
                "title": "Running containers",
                "title_size": "16",
                "title_align": "center",
                "time": {
                    "live_span": "1m"
                },
                "type": "query_value",
                "requests": [
                    {
                        "formulas": [
                            {
                                "formula": "query1"
                            }
                        ],
                        "conditional_formats": [
                            {
                                "comparator": ">=",
                                "palette": "white_on_green",
                                "value": 1
                            },
                            {
                                "comparator": "<",
                                "palette": "white_on_red",
                                "value": 1
                            }
                        ],
                        "response_format": "scalar",
                        "queries": [
                            {
                                "query": "max:otel.datadog_exporter.metrics.running{*}",
                                "data_source": "metrics",
                                "name": "query1",
                                "aggregator": "sum"
                            }
                        ]
                    }
                ],
                "autoscale": true,
                "precision": 0,
                "timeseries_background": {
                    "type": "area",
                    "yaxis": {
                        "include_zero": false
                    }
                }
            }
        },
        {
            "id": 11,
            "definition": {
                "title": "Running container change",
                "title_size": "16",
                "title_align": "center",
                "time": {
                    "live_span": "5m"
                },
                "type": "query_value",
                "requests": [
                    {
                        "formulas": [
                            {
                                "formula": "100 * (count_not_null(query2) / hour_before(count_not_null(query1)))"
                            }
                        ],
                        "conditional_formats": [
                            {
                                "comparator": ">=",
                                "palette": "white_on_green",
                                "value": 80
                            },
                            {
                                "comparator": ">",
                                "palette": "white_on_yellow",
                                "value": 50
                            },
                            {
                                "comparator": ">=",
                                "palette": "white_on_red",
                                "value": 0
                            }
                        ],
                        "response_format": "scalar",
                        "queries": [
                            {
                                "query": "sum:otel.datadog_exporter.metrics.running{*} by {aklhostname}",
                                "data_source": "metrics",
                                "name": "query2",
                                "aggregator": "last"
                            },
                            {
                                "query": "sum:otel.datadog_exporter.metrics.running{*} by {aklhostname}",
                                "data_source": "metrics",
                                "name": "query1",
                                "aggregator": "last"
                            }
                        ]
                    }
                ],
                "autoscale": false,
                "custom_unit": "%",
                "text_align": "center",
                "custom_links": [],
                "precision": 0
            }
        },
